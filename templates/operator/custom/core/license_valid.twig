        <i class="fa fa-5x fa-check-circle-o" style="color: #27ae60;"></i>

        <a href="{{ route('core.operator.dashboard') }}" class="button primary">
            {{ Lang.get('core.go_to_dashboard') }}
        </a>

    </div>
{% endblock %}